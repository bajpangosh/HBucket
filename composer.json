    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Bajpan Gosh",
            "email": "email@example.com"
        }
    ],
    "require": {
        "aws/aws-sdk-php": "^3.200"
    },
    "autoload": {
        "psr-4": {
            "HBucket\\": "includes/"
        }
    },
    "require-dev": {
        "yoast/wp-test-utils": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml.dist"
    }
